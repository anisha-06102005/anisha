import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int digit1 = num / 10;
        int digit2 = num % 10;
        int sum = digit1 + digit2;
        System.out.println("Bird said:" + num);
        System.out.println("Alice must go in path-" + sum);
    }
}
