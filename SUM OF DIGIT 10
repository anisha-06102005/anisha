import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt(), sum = 0; 
        if ((num < 100) || (num == 0)) {
            System.out.println("Invalid Input");
        } 
        else 
        {
            while (num > 0) {
                sum = sum + num % 10;
                num = num / 10;
            }
            System.out.println("Sum of digit is " + sum);
        }
    }
}
