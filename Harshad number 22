import java.util.Scanner;

public class type {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt(), sum = 0, n = num;
        while (n > 0) {
            sum = sum + n % 10;  
            n = n / 10;  
        }
        if (num % sum == 0) {
            System.out.println("Harshad Number");
        } else {
            System.out.println("Not Harshad Number");
        }
    }
}

